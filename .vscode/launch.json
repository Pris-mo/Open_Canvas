{
    "version": "0.2.0",
      
    "inputs": [
      {
        "id": "courseId",
        "type": "promptString",
        "description": "Canvas Course ID",
        "default": "3376"
      },
      {
        "id": "canvasUrl",
        "type": "promptString",
        "description": "Canvas base URL",
        "default": "https://learn.canvas.net"
      }
    ],
    "configurations": [
      {
        "name": "Run Canvas Crawler",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/canvas_crawler/canvas_crawler.py",
        "args": [
          "--course-id", "${input:courseId}",
          "--output-dir", "${workspaceFolder}/canvas_crawler",
          "--depth-limit", "15",
          "--canvas-url", "${input:canvasUrl}",
          "--verbose"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "justMyCode": true
      },
      {
        "name": "Run Canvas Crawler w/ External Links",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/canvas_crawler/canvas_crawler.py",
        "args": [
          "--course-id", "${input:courseId}",
          "--output-dir", "${workspaceFolder}/canvas_crawler",
          "--depth-limit", "15",
          "--canvas-url", "${input:canvasUrl}",
          "--include-external-links",
          "--verbose"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "justMyCode": true
      },
      {
        "name": "Run Current Python File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": true
      },
      {
        "name": "Run Pre Processor",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/pre_processer/run_conversion.py",
        "args": [
          "--source-root", "${workspaceFolder}/canvas_crawler/output/3376",
          "${workspaceFolder}/canvas_crawler/output/3376/pages",
          //"${workspaceFolder}/test_files/Accelerating-Sustainability-with-AI-2025.pdf",
          //"${workspaceFolder}/test_files/Anderson 1984.pdf",
          //"${workspaceFolder}/test_files/city-skyline.png",
          //"${workspaceFolder}/test_files"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "justMyCode": true
      },
      {
        "name": "Run chunker",
        "type": "debugpy",
        "request": "launch",
        "module": "chunker.cli",
        "args": [
          "--master-run", "${workspaceFolder}/runs/20260129_163239",
          "--json-output", "${workspaceFolder}/runs/20260129_163239/canvas/output/3376/json_output",
          "--include-separate-metadata",
          "--write-chunk-files"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "justMyCode": true,
        "cwd": "${workspaceFolder}"
        },      {
        "name": "Run Orchestrator (CLI)",
        "type": "debugpy",
        "request": "launch",
        "module": "orchestrator.cli",
        "args": [
          "--course-id", "3376",
          "--canvas-url", "https://learn.canvas.net",
          "--runs-root", "runs",
          "--crawler-verbose",
          "--conversion-verbose",
          "--enable-llm",
          "--model", "gpt-4o",
          "--skip-dirname", "json_output",
          "--skip-dirname", "locked",
          "--chunking-enabled",
          "--write-chunk-files"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "justMyCode": true,
        "cwd": "${workspaceFolder}"
      }
      
    ]
  }